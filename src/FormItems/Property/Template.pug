form-item-with-label(
    v-bind:label='label'
    v-bind:validation_errors='validation_errors'
    v-bind:saved_template_names='saved_template_names'
)
    div(
        v-bind:id='formItemId'
    )
        web-designer-text-input(
            label='name'
            v-bind:value.sync='propertyName'
        )
        web-designer-text-input(
            label='value'
            v-bind:value.sync='propertyValue'
        )
